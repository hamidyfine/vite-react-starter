{
  "name": "vite-react-starter-kit",
  "private": true,
  "version": "1.0.0",
  "build": "./build",
  "type": "module",
  "scripts": {
    "bootstrap": "yarn && yarn translation && yarn build",
    "dev": "vite --port=4000",
    "build": "yarn translation && tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --fix",
    "translation": "npx ts-node --esm ./scripts/translation.ts && npx eslint --ext .ts src/locales/index.ts --fix --quiet"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "i18next": "^23.2.11",
    "i18next-browser-languagedetector": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-redux": "^8.1.1",
    "react-router-dom": "6",
    "sass": "^1.64.1"
  },
  "devDependencies": {
    "@redux-devtools/core": "^3.13.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.45.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.27",
    "stylelint": "^15.10.2",
    "stylelint-config-hudochenkov": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-eslint": "^1.8.1",
    "web-vitals": "^3.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
